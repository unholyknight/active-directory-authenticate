{
    "name": "unholyknight/active-directory-authenticate",
    "description": "CakePHP plugin for Active Directory authentication over LDAP.",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "authenticate", "ldap", "ad"],
    "homepage": "http://github.com/unholyknight/active-directory-authenticate",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Lorentz",
            "homepage": "http://ericlorentz.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "cakephp/cakephp": "~3.0",
        "adldap2/adldap2": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.3"
    },
    "support": {
        "issues": "http://github.com/unholyknight/active-directory-authenticate/issues",
        "source": "http://github.com/unholyknight/active-directory-authenticate"
    },
    "autoload": {
        "psr-4": {
            "ActiveDirectoryAuthenticate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActiveDirectoryAuthenticate\\Test\\": "tests"
        }
    }
}
