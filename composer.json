        {
            "name": "Eric Lorentz",
            "homepage": "http://ericlorentz.com",
            "role": "Author"
        },
        {
            "name": "JarosÅ‚aw Milewski",
            "homepage": "https://milewski.it",
            "role": "Developer, bug eliminator"
        },
        {
            "name": "Alysson Azevedo",
            "homepage": "https://github.com/alysson-azevedo",
            "role": "Contributor"
        }
